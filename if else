import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      Scanner Sc = new Scanner(System.in );
      int n = Sc.nextInt();

      
        if(1<=n)
        {
        System.out.println("Positive Number");
        }
        else if (n<0)
        {
          System.out.println("Negative Number");
        }
        else
        {
          System.out.println("zero");
        }
}
}
